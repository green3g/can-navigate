{{>header}}

{{#if pagePromise.isPending}}

    <p style="text-align:center">
        <i class="fa fa-spinner fa-spin fa-3x fa-fw"></i>
        <span class="sr-only">Loading...</span>
    </p>

{{else}}
    {{#if pagePromise.isResolved}}
        {{>pageData.render}}
    {{else}}
        {{#if pagePromise.isRejected}}
            {{>error}}
        {{/if}}
    {{/if}}
{{/if}}

{{>footer}}